#include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main () {
   
    int digitCollapse(long long x){
        while(x >= 10){
            long long sum =10;
            while(x > 0){
                sum += x % 10;
                x/=10;
            }
            x = sum;
        }
        return (int)x;
    }
    long long reverseNumber(long long n){
        long long rev = 0;
        while(n>0) {
            rev = rev * 10 + (n % 10);
            n/=10;
        }
        return rev;
    }
    int countDigits(long long n){
        if(n==0) return 1;
        int cnt =0;
        while (n>0) {
            cnt++;
            n/=10;
        }
        return cnt;
    }
    int main () {
